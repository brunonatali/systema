{
    "name": "brunonatali/systema",
    "description": "PHP modularity I/O system 4 communication.",
    "license": "MIT",
    "require": {
			"php": ">=7.3",
			"brunonatali/socket": "dev-master",
			"brunonatali/stream": "dev-master",
			"brunonatali/event-loop": "dev-master"
	},
	"require-dev": {
		"clue/block-react": "^1.2",
		"phpunit/phpunit": "^6.4 || ^5.7 || ^4.8.35"
	},
	"autoload": {
		"psr-4": {
			"BrunoNatali\\SysTema\\Defines\\": "src/defines",
			"BrunoNatali\\SysTema\\Managers\\": "src/manager",
			"BrunoNatali\\SysTema\\Telemetry\\": "src/telemetry",
			"BrunoNatali\\SysTema\\Misc\\": "src/misc",
			"BrunoNatali\\SysTema\\Group\\": "src/group",
			"BrunoNatali\\SysTema\\Things\\": "src/things",
			"BrunoNatali\\SysTema\\Sockets\\": "src/sockets",
			"BrunoNatali\\SysTema\\WebSockets\\": "src/webSockets",
			"BrunoNatali\\SysTema\\Xkg\\": "src/xkg"
		}
	},
    "minimum-stability": "dev",
    "prefer-stable": true
}
